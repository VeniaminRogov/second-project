{% extends './base.html.twig' %}

{% block title %}Add user{% endblock %}
{% block pageTitle %}New user{% endblock %}

{% block content %}
    <div class="form d-flex justify-content-between">
        {{ form_start(form) }}
            {{ form_row(form.email) }}
            {{ form_row(form.password) }}
            {{ form_row(form.roles) }}
            {{ form_row(form.save) }}
        {{ form_end(form) }}
    </div>
{% endblock %}