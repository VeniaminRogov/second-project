{% trans_default_domain 'admin' %}

{% extends './base.html.twig' %}

{% block title %}{{ title }}{% endblock %}
{% block pageTitle %}{{ title }}{% endblock %}
{% block content %}
    <div class="form">
        {{ form_start(form) }}
            {{ form_row(form.email) }}
            {{ form_row(form.password) }}
            {{ form_row(form.roles) }}
            {{ form_row(form.is_verified) }}
            {{ form_row(form.save) }}
        {{ form_end(form) }}
    </div>
{% endblock %}